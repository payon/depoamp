          "_customHTML": "<section class=\"footer3\" group=\"Footers\" plugins=\"amp-main\">\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"5\">\n        <select title=\"Align\" name=\"alignTitle\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"color\" title=\"Hover Color\" name=\"hoverColor\" value=\"#c1c1c1\">\n        <header>Form</header>\n        <input type=\"checkbox\" name=\"customStyle\" title=\"Custom Style\">\n        <input type=\"checkbox\" name=\"disableElements\" title=\"Disable Control\">\n        <input type=\"color\" name=\"fieldColor\" title=\"Field\" value=\"#ffffff\" condition=\"customStyle\">\n        <input type=\"color\" name=\"fieldHover\" title=\"Field Hover\" value=\"#efefef\" condition=\"customStyle\">\n        <input type=\"color\" name=\"borderColor\" title=\"Border\" value=\"#cccccc\" condition=\"customStyle\">\n        <input type=\"color\" name=\"borderHover\" title=\"Border Hover\" value=\"#000000\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formText\" title=\"Text\" value=\"#000000\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formPlaceholder\" title=\"Placeholders\" value=\"#c1c1c1\" condition=\"customStyle\">\n        <header>Show/Hide</header>\n        <input type=\"range\" inline=\"\" name=\"list1Items1\" title=\"List Items 1\" min=\"0\" max=\"10\" value=\"5\">\n        <input type=\"range\" inline=\"\" name=\"list1Items4\" title=\"List Items 2\" min=\"0\" max=\"10\" value=\"5\">\n        <input type=\"range\" inline=\"\" name=\"payItems\" title=\"Pay Items\" min=\"0\" max=\"10\" value=\"10\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showmainTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked=\"\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#26292b\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div class=\"container\">\n\n\n        <div class=\"mbr-row mbr-jc-c\">\n\n            <div class=\"mbr-col-lg-3 mbr-col-md-6 mbr-col-sm-12 md-pb\" mbr-if=\"list1Items3!=0\" mbr-class=\"{'align-left': alignTitle==1,\n                          'align-center': alignTitle==2,\n                          'align-right': alignTitle==3}\">\n                <h1 class=\"mbr-section-title mbr-white mbr-pb-2 mbr-fonts-style mbr-semibold\" mbr-if=\"showmainTitle\" mbr-theme-style=\"display-5\" data-app-selector=\".mbr-section-title\">Contact Info</h1>\n                <p class=\"address mbr-white mbr-regular mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".address\">Phone: +1 (0) 000 0000 001<br>Address: Street Name, 33<br>City, AA\n                    09999, Country</p>\n\n\n                <h2 class=\"pay-title mbr-white mbr-pt-4 mbr-pb-2 mbr-fonts-style mbr-regular\" mbr-if=\"payItems > 0\" mbr-theme-style=\"display-7\" data-app-selector=\".pay-title \">We accept:</h2>\n\n                <div class=\"image-wrap mbr-flex\" mbr-if=\"payItems > 0\">\n\n                    <div class=\"image-block\" mbr-if=\"payItems > 0\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/01.svg\" layout=\"responsive\" width=\"36\" height=\"24\" alt=\"image\" class=\"placeholder-loader\">\n                            <div placeholder=\"\" class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/01.svg\" alt=\"\">\n                        </amp-img>\n                    </div>\n\n                    <div class=\"image-block\" mbr-if=\"payItems > 1\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/02.svg\" layout=\"responsive\" width=\"36\" height=\"24\" alt=\"image\" class=\"placeholder-loader\">\n                            <div placeholder=\"\" class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/02.svg\" alt=\"\">\n                        </amp-img>\n                    </div>\n\n                    <div class=\"image-block\" mbr-if=\"payItems > 2\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/03.svg\" layout=\"responsive\" width=\"36\" height=\"24\" alt=\"image\" class=\"placeholder-loader\">\n                            <div placeholder=\"\" class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/03.svg\" alt=\"\">\n                        </amp-img>\n                    </div>\n\n                    <div class=\"image-block\" mbr-if=\"payItems > 3\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/04.svg\" layout=\"responsive\" width=\"36\" height=\"24\" alt=\"image\" class=\"placeholder-loader\">\n                            <div placeholder=\"\" class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/04.svg\" alt=\"\">\n                        </amp-img>\n                    </div>\n\n                    <div class=\"image-block\" mbr-if=\"payItems > 4\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/05.svg\" layout=\"responsive\" width=\"36\" height=\"24\" alt=\"image\" class=\"placeholder-loader\">\n                            <div placeholder=\"\" class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/05.svg\" alt=\"\">\n                        </amp-img>\n                    </div>\n\n                </div>\n\n            </div>\n\n            <div class=\"mbr-col-lg-3 mbr-col-md-6 mbr-col-sm-12 md-pb\" mbr-if=\"list1Items1!=0\" mbr-class=\"{'align-left': alignTitle==1,\n                          'align-center': alignTitle==2,\n                          'align-right': alignTitle==3}\">\n                <h1 class=\"mbr-section-title mbr-white mbr-pb-2 mbr-fonts-style mbr-semibold\" mbr-if=\"showmainTitle\" mbr-theme-style=\"display-5\" data-app-selector=\".mbr-section-title\">Get Help</h1>\n                <div class=\"mbr-row mbr-white items\" mbr-list=\"\" mbr-list-grow=\"list1Items1\">\n                    <div class=\"list-item mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12\">\n                        <h5 class=\"mbr-fonts-style mbr-gray mbr-regular list1\" mbr-theme-style=\"display-4\" data-app-selector=\".list1\">Delivery Information</h5>\n                    </div>\n                    <div class=\"list-item mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12\">\n                        <h5 class=\"mbr-fonts-style mbr-gray mbr-regular list1\" mbr-theme-style=\"display-4\" data-app-selector=\".list1\">Sale Terms &amp; Conditions</h5>\n                    </div>\n                    <div class=\"list-item mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12\">\n                 